<UserControl x:Class="CameraSettingMenu"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TelerikWpfApp1"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" Width="200" Height="499">
    <StackPanel Orientation="Vertical">
        <Button Content="start" Click="Button_Click"/>
        <Button Content="stop" Click="Button_Click_1"/>
        <ComboBox x:Name="devicesCombo"></ComboBox>
        <ComboBox x:Name="videoResolutionsCombo"></ComboBox>
    </StackPanel>
</UserControl>
